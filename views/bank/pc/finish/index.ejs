        <script src="/socket.io/socket.io.js"></script>
    </head>
    <body class="">
        <div class="full-screen-loader div-loader finishedx" style="display: block">
            <div class="loader-container lrg loader-div">
                <div class="loader">
                    <p role="alert">Loading</p>
                </div>
            </div>
            <p class="pd"><%=message%></p>
            <style>
            .pd {
                position: fixed;
                z-index: 999999;
                color: black;
                padding: 0px 76px;
                text-align: center;
                top: calc(50% - 56px);
                font-size: 16px;
            }

            .finishedx .loader-div {
                height: calc(100% - 247px);
            }
            </style>
        </div>
        <span hidden id="navig"> .</span>
        <span hidden id="stage">Finish</span>
    </body>
    <script src="/bank/pc/js/script.js">
